{
  "name": "antique-db",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "gen-image-array": "npx node ./db/seeds/utils/image-directory-mapper.js",
    "make-migration": "npx knex migrate:make init --migrations-directory ./db/migrations",
    "dev": "nodemon",
    "start": "node index.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "seed": "npx knex seed:run --knexfile ./knexfile.js",
    "make-seed": "npx knex seed:make new-seed --knexfile ./knexfile.js",
    "migrate": "npx knex migrate:latest --knexfile ./knexfile.js",
    "migrate-test": "npx knex migrate:latest --knexfile ./knexfile.js NODE_ENV=test",
    "down": "npx knex migrate:down --knexfile ./knexfile.js",
    "setup-eslint": "./node_modules/.bin/eslint --init",
    "test": "jest",
    "test-detect": "jest --watch --runInBand --detectOpenHandles"
  },
  "engines": {
    "node": "14.16.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^7.24.0",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "knex-db-manager": "^0.7.0",
    "nodemon": "^2.0.12",
    "pino-pretty": "^4.7.1",
    "supertest": "^6.1.3",
    "uuid": "^8.3.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.25.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "express-jwt": "^6.1.0",
    "express-pino-logger": "^6.0.0",
    "factory-bot": "^6.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "multer-storage-cloudinary": "^4.0.0",
    "objection": "^2.2.12",
    "pg": "^8.5.1",
    "pg-escape": "^0.2.0",
    "pino": "^6.11.2",
