{
    "env": {
        "node": true,
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "node/no-process-env":["off", "always"],
        "semi":"error",
        "indent": [
          "warn",
          2
        ]
    }
}
